execute pathogen#infect()

set encoding=utf-8
set history=700

syntax enable
colorscheme mymonokai

"set lines=50 columns=50 

filetype on
filetype plugin on
filetype indent on
syntax on

set number
set ruler
set hlsearch
set incsearch

set ts=2 sw=2 sts=2
set expandtab

set ai
set si

set guioptions-=M "menubar
set guioptions-=T "toolbar
set guioptions-=r "scrollbar

set clipboard=unnamed
